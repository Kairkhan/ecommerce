<%-include("header")%>
<form action="/add" method="POST">
  <div class="form-group">
    <label for="exampleFormControlInput1">Title</label>
    <input type="text" class="form-control"  name= "title" autocomplete="off">
  </div>
  <div class="form-group">
    <label for="image">Image</label>
    <input type="text" class="form-control img-url"  name="imageURL1" placeholder="URL" autocomplete="off">
    <input type="text" class="form-control img-url"  name="imageURL2" placeholder="URL" autocomplete="off">
    <input type="text" class="form-control img-url"  name="imageURL3" placeholder="URL" autocomplete="off">
  </div>
  <div class="form-group">
    <label for="exampleFormControlTextarea1">Description</label>
    <textarea class="form-control" name="description" rows="3"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="addButton">Add</button>
</form>
<%-include("footer")%>
