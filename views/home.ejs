<%- include("header")%>
    <div class="row row-cols-1 row-cols-md-3">
    <%cards.forEach(function(card){%>
      <div class="col mb-4">
        <a href="/good">
        <div class="card">
          <img src="images/template.png" class="card-img-top" alt="images/template.png">
          <div class="card-body">
            <h5 class="card-title"><%=card.title%></h5>
            <p class="card-text"><%=card.description%></p>
          </div>
        </div>
        </a>
      </div>
      <%})%>
    </div>
<%-include("footer")%>
